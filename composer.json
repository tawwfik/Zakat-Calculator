{
    "name": "tawwfik/zakat-calculator",
    "description": "A Laravel package for calculating zakat on money, gold, and silver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tawfik",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
       "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/config": "^9.0|^10.0",
        "illuminate/cache": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0|^8.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Tawfik\\ZakatCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tawfik\\ZakatCalculator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tawfik\\ZakatCalculator\\ZakatServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}