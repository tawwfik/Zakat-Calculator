{
    "name": "tawwfik/zakat-calculator",
    "description": "A Laravel package for calculating zakat on money, gold, and silver.",
    "version": "1.1.0",
    "keywords": [
        "laravel",
        "zakat",
        "calculator",
        "gold",
        "silver",
        "money",
        "islamic"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tawfik",
            "email": "taww002016@gmail.com"
        }
    ],
    "homepage": "https://github.com/tawwfik/Zakat-Calculator",
    "support": {
        "issues": "https://github.com/tawwfik/Zakat-Calculator/issues",
        "source": "https://github.com/tawwfik/Zakat-Calculator"
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/config": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Tawfik\\ZakatCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tawfik\\ZakatCalculator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tawfik\\ZakatCalculator\\ZakatServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}